    "from IPython.display import display, Markdown\n",
    "\n",
    "# Resolução simbólica com SymPy\n",
    "display(Markdown(\"### **Verificação Simbólica para** $y' + y = e^{-x}$:\"))\n",
    "\n",
    "ode_1a = sp.Eq(y(x).diff(x) + y(x), sp.exp(-x))\n",
    "solucao_1a = sp.dsolve(ode_1a, y(x))\n",
    "\n",
    "display(solucao_1a)"
